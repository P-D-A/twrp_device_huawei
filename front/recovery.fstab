# mount point fstype device

/system      ext4   /dev/block/platform/omap/omap_hsmmc.1/by-name/system
/cache       auto   /dev/block/platform/omap/omap_hsmmc.1/by-name/cache
/data        auto   /dev/block/platform/omap/omap_hsmmc.1/by-name/userdata    length=-16384
/misc        emmc   /dev/block/platform/omap/omap_hsmmc.1/by-name/misc
/boot        emmc   /dev/block/platform/omap/omap_hsmmc.1/by-name/boot
/recovery    emmc   /dev/block/platform/omap/omap_hsmmc.1/by-name/recovery
/bootloader  emmc   /dev/block/platform/omap/omap_hsmmc.1/by-name/bootloader
/xloader     emmc   /dev/block/platform/omap/omap_hsmmc.1/by-name/xloader
/extSdCard   auto   /dev/block/mmcblk1p1   /dev/block/mmcblk1                 flags=display="Micro SDcard";storage;wipeingui;removable
/cust        ext4   /dev/block/platform/omap/omap_hsmmc.1/by-name/cust        flags=display="cust";backup=1;wipeingui;usermrf
/usb-ext     auto   /devices/platform/omap/musb-omap2430/musb-hdrc/usb1       flags=display="OTG device";storage;removable
